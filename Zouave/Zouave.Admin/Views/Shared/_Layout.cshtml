<!DOCTYPE html>
<html lang="zh-cn">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewBag.Title</title>
  @Styles.Render("~/Content/admin")
  @RenderSection("css", false)
  <script>
    (function (i, s, o, g, r, a, m) {
      i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
        (i[r].q = i[r].q || []).push(arguments)
      }, i[r].l = 1 * new Date(); a = s.createElement(o),
		  m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
    ga('create', 'UA-44987299-1', 'bootstrap-hunter.com');
    ga('send', 'pageview');
  </script>

  <!--[if lt IE 9]>

  <![endif]-->

</head>
<body data-color="grey" class="flat">
  <div id="wrapper">
    @Html.Partial("_Header")
    @Html.Partial("_UserNav")
    @Html.Partial("_Switcher")
    @Html.Partial("_Sidebar")

    <div id="content">
      <div id="content-header" class="mini">
        <h1>仪表盘</h1>
        <ul class="mini-stats box-3">
          <li>
            <div class="left sparkline_bar_good"><span>2,4,9,7,12,10,12</span>+10%</div>
            <div class="right">
              <strong>36094</strong>
              Visits
            </div>
          </li>
          <li>
            <div class="left sparkline_bar_neutral"><span>20,15,18,14,10,9,9,20</span>0%</div>
            <div class="right">
              <strong>1433</strong>
              Users
            </div>
          </li>
          <li>
            <div class="left sparkline_bar_bad"><span>3,5,9,7,12,20,10</span>+50%</div>
            <div class="right">
              <strong>8650</strong>
              Orders
            </div>
          </li>
        </ul>
      </div>
      <div id="breadcrumb">
        <a href="#" title="回到主页" class="tip-bottom"><i class="fa fa-home"></i> 主页</a>
        <a href="#" class="current">仪表盘</a>
      </div>
      <div class="container-fluid">
        @RenderBody()

      </div>
    </div>
    @Html.Partial("_Footer")
  </div>
  @Scripts.Render("~/bundles/jquery")
  @Scripts.Render("~/bundles/jqueryval")
  @Scripts.Render("~/bundles/jqueryui")
  @Scripts.Render("~/bundles/jqueryplugins")
  @Scripts.Render("~/bundles/bootstrap")

  @Scripts.Render("~/bundles/unicorn")

  @*<script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/excanvas.min.js") "></script>
    <script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/jquery.min.js") "></script>
    <script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/jquery-ui.custom.js") "></script>
    <script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/bootstrap.min.js") "></script>


    <script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/jquery.flot.min.js") "></script>
    <script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/jquery.flot.resize.min.js") "></script>
    <script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/jquery.sparkline.min.js") "></script>

    <script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/fullcalendar.min.js") "></script>
    <script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/jquery.nicescroll.min.js") "></script>
    <script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/unicorn.js") "></script>
    <script type="text/javascript" src="@Scripts.Url("~/Content/unicorn/js/unicorn.dashboard.js") "></script>*@
  @RenderSection("js", false)
</body>
</html>
